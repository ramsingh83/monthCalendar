{
  "name": "monthcalendar",
  "version": "0.0.1",
  "description": "Angular app for date range month calendar",
  "author": "Ram Singh",
  "license": "UNLICENSED",
  "main": "src/main.module.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "lint": "eslint ./src --ignore-pattern shared*.js *spec.js --cache",
    "checkstyle": "eslint ./src --ignore-pattern shared*.js *spec.js --cache -f checkstyle > reports/cs_result.xml; exit 0;",
    "prebuild": "npm run clean",
    "build": "webpack --bail --progress --profile",
    "dev": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "e2e": "protractor protractor.conf.js",
    "start": "npm run dev",
    "deploy": "webpack -p --bail",
    "stats": "webpack --profile --json > stats.json",
    "docs": "mkdirp docs && jsdoc src -r -d docs",
    "clean": "rimraf build",
    "clean:docs": "rimraf /docs/*",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "dependencies": {
    "angular": "1.6.5",
    "angular-aria": "1.6.5",
    "angular-moment": "^1.0.1",
    "angular-route": "1.6.5",
    "angular-sanitize": "1.6.5",
    "lodash": "4.17.4",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "angular-mocks": "1.6.5",
    "angular-resource": "1.6.5",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "chromedriver": "^2.27.2",
    "compass-mixins": "^0.12.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.7.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "jsdoc": "^3.4.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "ng-annotate-loader": "^0.2.0",
    "ng-cache-loader": "0.0.21",
    "ngtemplate-loader": "^1.3.1",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.13.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "2.1.7",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.0.0",
    "pre-commit": "^1.1.3",
    "protractor": "^5.1.0",
    "purify-css": "^1.2.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-validator": "^2.2.7"
  }
}
